<p id="notice"><%= notice %></p>

<h1>Listing Skills To Profiles</h1>

<table>
  <thead>
    <tr>
      <th>Proficency</th>
      <th>Note</th>
      <th>Add1</th>
      <th>Add2</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @skills_to_profiles.each do |skills_to_profile| %>
      <tr>
        <td><%= skills_to_profile.proficency %></td>
        <td><%= skills_to_profile.note %></td>
        <td><%= skills_to_profile.add1 %></td>
        <td><%= skills_to_profile.add2 %></td>
        <td><%= link_to 'Show', skills_to_profile %></td>
        <td><%= link_to 'Edit', edit_skills_to_profile_path(skills_to_profile) %></td>
        <td><%= link_to 'Destroy', skills_to_profile, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Skills to profile', new_skills_to_profile_path %>
