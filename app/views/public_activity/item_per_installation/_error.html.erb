<li class="list-group-item list-group-item-warning">
    <% if a.owner %>
        <%= a.created_at.to_formatted_s(:long) %> - <%= a.owner.email %> completed  <strong><%=a.trackable.rel_template_item.name %></strong> with error
    <% end %>
    <% if !a.owner %>
        <%= a.created_at.to_formatted_s(:long) %> -Someone completed <strong><%=a.trackable.rel_template_item.name %></strong> with error 
    <% end %>
</li>