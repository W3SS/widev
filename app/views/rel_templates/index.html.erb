<p id="notice"><%= notice %></p>

<h1>Listing Rel Templates</h1>

 <div class="table-responsive">
  <table class="table">
  <thead>
    <tr>
      <th>Name</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @rel_templates.each do |rel_template| %>
      <tr>
        <td><%= rel_template.name %></td>
        <td><%= link_to 'Show', rel_template %></td>
        <td><%= link_to 'Edit', edit_rel_template_path(rel_template) %></td>
        <td><%= link_to 'Destroy', rel_template, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>
   </div>
<br>

<%= link_to 'New Rel template', new_rel_template_path %>
